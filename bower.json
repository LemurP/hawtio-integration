{
  "name": "hawtio-integration",
  "keywords": [
    "hawtio"
  ],
  "main": [
    "dist/hawtio-integration.css",
    "dist/hawtio-integration.js"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "libs",
    "test",
    "tests",
    "plugins"
  ],
  "dependencies": {
    "hawtio-jmx": "~3.0.0",
    "hawtio-preferences": "~3.0.0",
    "dagre": "https://github.com/hawtio/dagre.git#3825485552cb2ebc79afab24f9a90f14c7e1e2df"
  },
  "devDependencies": {
    "hawtio-core-dts": "~3.0.0",
    "hawtio-extension-service": "~3.0.0"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    },
    "angular-resizable": {
      "main": [
        "src/angular-resizable.css",
        "src/angular-resizable.js"
      ]
    },
    "datatables": {
      "main": [
        "media/css/jquery.dataTables.css",
        "media/images/sort_asc.png",
        "media/images/sort_asc_disabled.png",
        "media/images/sort_both.png",
        "media/images/sort_desc.png",
        "media/images/sort_desc_disabled.png"
      ]
    }
  },
  "resolutions": {
    "angular": "~1.5.0",
    "jquery": "~2.1.0"
  }
}
